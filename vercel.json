{
    "version": 2,
    "builds": [
        {
            "src": "app_web.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": 50
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app_web.py"
        }
    ],
    "env": {
        "PYTHON_VERSION": "3.10.5"
    }
}

