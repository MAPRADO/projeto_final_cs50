{
    "version": 2,
    "builds": [
        {
            "src": "build.sh",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        },
        {
            "src": "app_web.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": 50
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app_web.py"
        }
    ],
    "env": {
        "PYTHON_VERSION": "3.10.5"
    },
    "installCommand": "apt-get update && apt-get install -y python3-pip && pip3 install -r requirements.txt"

}

